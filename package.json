{
  "name": "vitest-explorer",
  "displayName": "vitest explorer",
  "description": "Testing adaptor for vitest",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vitest-explorer.configureTest",
    "workspaceContains:**/vitest.config.*s",
    "workspaceContains:**/vite.config.*s"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vitest-explorer.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test:pure": "vitest run"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.6",
    "vitest": "^0.6.0"
  },
  "dependencies": {
    "@babel/parser": "^7.17.3",
    "@babel/types": "^7.17.0",
    "execa": "^5.1.0",
    "fs-extra": "^10.0.1",
    "mighty-promise": "^0.0.8"
  }
}
